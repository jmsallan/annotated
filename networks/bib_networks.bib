@article{Bagler2008,
	abstract = {Transportation infrastructure of a country is one of the most important indicators of its economic growth. Here we study the Airport Network of India (ANI) which represents India's domestic civil aviation infrastructure as a complex network. We find that ANI, a network of domestic airports connected by air links, is a small-world network characterized by a truncated power-law degree distribution and has a signature of hierarchy. We investigate ANI as a weighted network to explore its various properties and compare them with their topological counterparts. The traffic in ANI, as in the World-wide Airport Network (WAN), is found to be accumulated on interconnected groups of airports and is concentrated between large airports. In contrast to WAN, ANI is found to be having disassortative mixing which is offset by the traffic dynamics. The analysis indicates possible mechanism of formation of a national transportation network, which is different from that on a global scale. {\textcopyright} 2008 Elsevier Ltd. All rights reserved.},
	annote= "{In this paper we are learned about the properties of the Airport Network of India (ANI). Similarly to other airport networks, the ANI has the small-world property and a power-law degree distribution. Clustering coefficient decays as node degree increases, showing evidence of a hierarchical organization of this network. ANI is also analyzed as a weighted network, being weights the number of flights between each pair of connected airports. Node strength is correlated with node degree similarly to the WAN, as found in \cite{Barrat2004}.}",
	archivePrefix = {arXiv},
	arxivId = {cond-mat/0409773},
	author = {Bagler, Ganesh},
	doi = {10.1016/j.physa.2008.01.077},
	eprint = {0409773},
	file = {:Users/jmsallan/Desktop/1-s2.0-S0378437108001131-main.pdf:pdf},
	isbn = {0378-4371},
	issn = {03784371},
	journal = {Physica A: Statistical Mechanics and its Applications},
	keywords = {Airport network,Complex network,Transportation},
	mendeley-groups = {cnbook},
	month = {may},
	number = {12},
	pages = {2972--2980},
	primaryClass = {cond-mat},
	title = {{Analysis of the airport network of India as a complex weighted network}},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0378437108001131},
	volume = {387},
	year = {2008}
}

@article{DaRocha2009,
	abstract = {The aviation sector is profitable, but sensitive to economic fluctuations, geopolitical constraints and governmental regulations. As for other means of transportation, the relation between origin and destination results in a complex map of routes, which can be complemented by information associated to the routes themselves, for instance, frequency, traffic load or distance. The theory of networks provides a natural framework to investigate dynamics on the resulting structure. Here, we investigate the structure and evolution of the Brazilian Airport Network (BAN) for several quantities: routes, connections, passengers and cargo. Some structural features are in accordance with previous results of other airport networks. The analysis of the evolution of the BAN shows that its structure is dynamic, with changes in the relative relevance of some airports and routes. The results indicate that the connections converge to specific routes. The network shrinks at the route level but grows in number of passengers and amount of cargo, which more than doubled during the period studied.},
	annote= "{Authors investigate the structure and evolution of the Brazilian Airport Network (BAN) for several quantities: routes, connections, passengers and cargo. The analysis of the evolution of the BAN in the period 1995-2006 suggests that despite of small fluctuations in some structural properties, the network has undergone a strong rewiring dynamics in the considered period. What in principle might appear as a slowdown of the aviation sector, in fact is a reorganization of the structure according to changing demand. Along the years, the BAN presents the small-world property, and an exponential degree distribution. Network shrinks in nodes and number of connections although passengers and cargo traffic has raised.}",
	archivePrefix = {arXiv},
	arxivId = {0804.3081},
	author = {da Rocha, Luis E C},
	doi = {10.1088/1742-5468/2009/04/P04020},
	eprint = {0804.3081},
	file = {:Users/jmsallan/Desktop/0804.3081.pdf:pdf},
	isbn = {1742-5468},
	issn = {1742-5468},
	journal = {Journal of Statistical Mechanics: Theory and Experiment},
	keywords = {Communication,Dynamics of supply and production networks,Network dynamics,Socio-economic networks,Supply and information networks},
	mendeley-groups = {cnbook},
	month = {apr},
	number = {04},
	pages = {P04020},
	pmid = {9562380},
	title = {{Structural evolution of the Brazilian airport network}},
	url = {http://stacks.iop.org/1742-5468/2009/i=04/a=P04020?key=crossref.56303256b5299fdcdc0b4f312b4ef205},
	volume = {2009},
	year = {2009}
}

@article{Du2017b,
	abstract = {Due to rapid development in the past decade, air transportation system has attracted considerable research attention from diverse communities. While most of the previous studies focused on airline networks, here we systematically explore the robustness of the Chinese air route network, and identify the vital edges which form the backbone of Chinese air transportation system. Specifically, we employ a memetic algorithm to minimize the network robustness after removing certain edges, and hence the solution of this model is the set of vital edges. Counterintuitively, our results show that the most vital edges are not necessarily the edges of the highest topological importance, for which we provide an extensive explanation from the microscope view. Our findings also offer new insights to understanding and optimizing other real-world network systems.},
	annote = {The object of analysis of this paper is the Chinese air route network. Its nodes are waypoints and its edges air route segments connecting them. This is the network where real flight operations take place. This network is quite different from airport networks: it has low clustering coefficient, large average shortest path length and exponential degree distribution. This properties come from the spatial properties of the network. Authors investigate the robustness of this network to edge isolation detecting vital edges with a memetic algorithm. They find that the most vital edges are not the ones with highest topological importance (defined by edge betweenness).},
	archivePrefix = {arXiv},
	arxivId = {1608.00142},
	author = {Du, Wenbo and Liang, Boyuan and Yan, Gang and Lordan, Oriol and Cao, Xianbin},
	doi = {10.1016/j.cja.2016.12.001},
	eprint = {1608.00142},
	file = {:Users/jmsallan/Documents/mendeley/Du et al/Chinese Journal of Aeronautics/Du et al. - 2017 - Identifying vital edges in Chinese air route network via memetic algorithm.pdf:pdf},
	issn = {10009361},
	journal = {Chinese Journal of Aeronautics},
	keywords = {Air route network,Air transport network,Memetic algorithm,Robustness,Vital edges},
	mendeley-groups = {cnbook,olordan},
	month = {feb},
	number = {1},
	pages = {330--336},
	publisher = {Chinese Society of Aeronautics and Astronautics},
	title = {{Identifying vital edges in Chinese air route network via memetic algorithm}},
	url = {http://dx.doi.org/10.1016/j.cja.2016.12.001 https://linkinghub.elsevier.com/retrieve/pii/S1000936116302163},
	volume = {30},
	year = {2017}
}

@article{Belkoura2016,
	abstract = {Complex network theory is a framework increasingly used in the study of air transport networks, thanks to its ability to describe the structures created by networks of flights, and their influence in dynamical processes such as delay propagation. While many works consider only a fraction of the network, created by major airports or airlines, for example, it is not clear if and how such sampling process bias the observed structures and processes. In this contribution, we tackle this problem by studying how some observed topological metrics depend on the way the network is reconstructed, i.e. on the rules used to sample nodes and connections. Both structural and simple dynamical properties are considered, for eight major air networks and different source datasets. Results indicate that using a subset of airports strongly distorts our perception of the network, even when just small ones are discarded; at the same time, considering a subset of airlines yields a better and more stable representation. This allows us to provide some general guidelines on the way airports and connections should be sampled.},
	annote = {This article reports how topological metrics depend on how is the airport network reconstructed, that is, the rules used to sample nodes and connections. They have used several sources of route data: the ALL-FT+ data set provided by the EUROCONTROL PRISME group, the On-Time Performance dataset provided by the Research and Innovative Technology Administration (RITA) of the US and OpenFlights. They find that using connections from a subset of airlines yields a more accurate representation than using a subset of airports, even when small airports are omitted.},
	author = {Belkoura, Seddik and Cook, Andrew and Pe{\~{n}}a, Jos{\'{e}} Maria and Zanin, Massimiliano},
	doi = {10.1016/j.tre.2016.07.013},
	file = {:Users/jmsallan/Documents/mendeley/Belkoura et al/Transportation Research Part E Logistics and Transportation Review/Belkoura et al. - 2016 - On the multi-dimensionality and sampling of air transport networks.pdf:pdf},
	issn = {13665545},
	journal = {Transportation Research Part E: Logistics and Transportation Review},
	keywords = {Air transport,Complex networks,Network topology},
	mendeley-groups = {cnbook},
	pages = {95--109},
	publisher = {Elsevier Ltd},
	title = {{On the multi-dimensionality and sampling of air transport networks}},
	url = {http://dx.doi.org/10.1016/j.tre.2016.07.013},
	volume = {94},
	year = {2016}
}

@inproceedings{Hossain2013,
	abstract = {An airport network is a fundamental component of an air transportation infrastructure and has to be designed in such a manner that failure of any of its arbitrary components should not cascade into a catastrophic event. Resilience analysis of the Airports Network can offer an insight into airport network readiness and response behaviour to any catastrophic event. A complex network analysis approach of such a network can offer an insight about its structure, performance and resilience under various levels of perturbation. In this paper, we present a complex network approach for measuring the performance and estimating the resilience of an airport network using the Australian Airports Network (AAN) as a case study. Real air traffic data for all domestic flights in the Australian airspace for the year 2011 is used to form the network. Resilience is then assessed under different failure scenarios. The complex network analysis reveals that the AAN can be classified as a scale- free small-world network and that its structure is resilient to random failures of airports (e.g., shutdowns) and random disruptions of flight paths (e.g., airways unavailable due to bad weather). It also indicates that the AAN remains connected and incurs minimal increase in travel times and reduced ‘reachability' when a majority of its nodes (airports) are removed or its edges (airways) become randomly unavailable. In the case of a targeted failure (a targeted isolated airport shutdown), the AAN is more sensitive to node failure by a descending order of degree as well as ‘betweenness centrality'.},
	annote = {This paper uses the Australian Airport Network (AAN) as a case study to measure the performance and to estimate the resilience of an airport network. In the literature review section, authors make a classification of short-time outages and long-time outages in airports. The AAN is modelled as a weighted network, with number of flights as weights. They perform a quite complete robustness analysis, with giant component and efficiency as performance indicators, and an analysis of node importance deleting one node and defining a rerouting algorithm.},
	author = {Hossain, Murad and Alam, Sameer and Rees, Tim and Abbass, Hussein},
	booktitle = {Australasian Transport Research Forum 2013 Proceedings},
	file = {:Users/jmsallan/Documents/mendeley/Hossain et al/Australasian Transport Research Forum 2013 Proceedings/Hossain et al. - 2013 - Australian Airport Network Robustness Analysis A Complex Network Approach.pdf:pdf},
	keywords = {complex network,network},
	mendeley-groups = {AirportNetworks,cnbook},
	number = {October},
	pages = {1--21},
	title = {{Australian Airport Network Robustness Analysis : A Complex Network Approach}},
	year = {2013}
}

@article{Sun2014,
	abstract = {In this research, we propose a new methodology to assess structural similarity of air navigation route systems in 58 countries. We identify functional dependencies among network metrics through regression analysis. We build a graph for the network metrics, with each metric as a node and a link existing if there is a functional dependency between two metrics. We find that the air navigation route systems in France and Italy are most similar. The air navigation route systems in Oceania ensemble common features of most countries and should be used as representatives for analysis of new air traffic management operational procedures.},
	annote = {The paper reports a structural similarity analysis of the air route networks of the 58 countries with largest air route networks. In air navigation route networks, nodes are air navigation points and links are route segments connecting air navigation points. Data is obtained from the European Aeronautical Information Service Database, EAD) provided by Eurocontrol. Contains a useful review on air route networks analysis and network comparison. The methodology used to compare air route networks needs to be refined. For instance, authors first discard Pearson correlation coefficients because variables are not normally distributed. Then, use $R^2$ of pairs of regression analysis as a measure of association. Other techniques used are clustering analysis and principal component analysis.},
	author = {Sun, Xiaoqian and Wandelt, Sebastian},
	doi = {10.1016/j.tre.2014.08.005},
	file = {:Users/jmsallan/Documents/mendeley/Sun, Wandelt/Transportation Research Part E Logistics and Transportation Review/Sun, Wandelt - 2014 - Network similarity analysis of air navigation route systems.pdf:pdf},
	issn = {13665545},
	journal = {Transportation Research Part E: Logistics and Transportation Review},
	keywords = {Air navigation route system,Air traffic management,Functional dependency,Network similarity},
	month = {oct},
	pages = {416--434},
	title = {{Network similarity analysis of air navigation route systems}},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S1366554514001458},
	volume = {70},
	year = {2014}
}

