	title = {{Exploring complex networks}},
	url = {http://www.nature.com/doifinder/10.1038/35065725 http://www.nature.com/articles/35065725},
	volume = {410},
	year = {2001}
}

@article{Barrat2004,
	abstract = {Networked structures arise in a wide array of different contexts such as technological and transportation infrastructures, social phenomena, and biological systems. These highly interconnected systems have recently been the focus of a great deal of attention that has uncovered and characterized their topological complexity. Along with a complex topological structure, real networks display a large heterogeneity in the capacity and intensity of the connections. These features, however, have mainly not been considered in past studies where links are usually represented as binary states, i.e., either present or absent. Here, we study the scientific collaboration network and the world-wide air-transportation network, which are representative examples of social and large infrastructure systems, respectively. In both cases it is possible to assign to each edge of the graph a weight proportional to the intensity or capacity of the connections among the various elements of the network. We define appropriate metrics combining weighted and topological observables that enable us to characterize the complex statistical properties and heterogeneity of the actual strength of edges and vertices. This information allows us to investigate the correlations among weighted quantities and the underlying topological structure of the network. These results provide a better description of the hierarchies and organizational principles at the basis of the architecture of weighted networks.},
	annote= "{The article analyzes complex networks whose edges have assigned weights representing the intensity of relationship between the nodes connecting them. Authors introduce metrics specific for complex weighted networks: node strength, weighted clustering coefficient and weighted average nearest-neighbors degree. Using two samples of the scientific citation network and the world airport network, they define a methodology for analyzing the architecture of complex weighted networks. They analyze the relationship between strength and degree and strength and betweenness, the distribution of weighted clustering coefficient as a function of node degree, and node strength correlation.}",
	author = {Barrat, Alain and Barth{\'{e}}lemy, M and Pastor-Satorras, R and Vespignani, A},
	doi = {10.1073/pnas.0400087101},
	file = {:home/jmsallan/Documentos/Mendeley Desktop/Barrat et al/Proceedings of the National Academy of Sciences of the United States of America/Barrat et al. - 2004 - The architecture of complex weighted networks.pdf:pdf},
	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	keywords = {Data Interpretation,Information Services,Publications,Research Personnel,Statistical,networks},
	mendeley-groups = {cnbook},
	mendeley-tags = {networks},
	month = {mar},
	number = {11},
	pages = {3747--52},
	pmid = {15007165},
	title = {{The architecture of complex weighted networks.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=374315{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {101},
	year = {2004}
}

@article{Newman2003,
	abstract = {We study assortative mixing in networks, the tendency for vertices in networks to be connected to other vertices that are like (or unlike) them in some way. We consider mixing according to discrete characteristics such as language or race in social networks and scalar characteristics such as age. As a special example of the latter we consider mixing according to vertex degree, i.e., according to the number of connections vertices have to other vertices: do gregarious people tend to associate with other gregarious people? We propose a number of measures of assortative mixing appropriate to the various mixing types, and apply them to a variety of real-world networks, showing that assortative mixing is a pervasive phenomenon found in many networks. We also propose several models of assortatively mixed networks, both analytic ones based on generating function methods, and numerical ones based on Monte Carlo graph generation techniques. We use these models to probe the properties of networks as their level of assortativity is varied. In the particular case of mixing by degree, we find strong variation with assortativity in the connectivity of the network and in the resilience of the network to the removal of vertices.},
	annote= "{Assortative mixing is a property initially defined for social networks. A social network is said to be assortative or have an assortative mixing if nodes tend to be connected to similar nodes, and dissortative when nodes tend to be connected to different nodes. Assortative mixing can be defined for discrete qualities (gender, race) or scalar properties such as age. A particularly important scalar property is node degree. Regarding node degree, social networks tend to be assortative, while technological tend to be dissortative. The paper includes the definition of an assortativity coefficient measure, and models that allow the generation of networks with diverse values of grades of dissortativy.}",
	archivePrefix = {arXiv},
	arxivId = {cond-mat/0209450},
	author = {Newman, M. E. J.},
	doi = {10.1103/PhysRevE.67.026126},
	eprint = {0209450},
	file = {:home/jmsallan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Newman - 2003 - Mixing patterns in networks.pdf:pdf},
	isbn = {1539-3755},
	issn = {1063-651X},
	journal = {Physical Review E},
	mendeley-groups = {cnbook},
	month = {feb},
	number = {2},
	pages = {026126},
	pmid = {12636767},
	primaryClass = {cond-mat},
	title = {{Mixing patterns in networks}},
	url = {https://link.aps.org/doi/10.1103/PhysRevE.67.026126},
	volume = {67},
	year = {2003}
}

@article{Lee2015,
	abstract = {Many real-world complex systems are best modeled by multiplex networks of interacting network layers. The multiplex network study is one of the newest and hottest themes in the statistical physics of complex networks. Pioneering studies have proven that the multiplexity has broad impact on the system's structure and function. In this Colloquium paper, we present an organized review of the growing body of current literature on multiplex networks by categorizing existing studies broadly according to the type of layer coupling in the problem. Major recent advances in the field are surveyed and some outstanding open challenges and future perspectives will be proposed.},
	annote= "{This is an organic review of statistical physics of multiplex networks. Multiplex networks is a type of network where the same set of nodes can be connected via than one more type of link. Each type of link is a network layer. The paper reports modelling of multiplex structure and multiplex network models. It includes also a review of problems of mutual percolation, robustness against attacks and complex contagion for multiplex networks with cooperative (interdependent) layer coupling and complementary layer coupling. The bibliography section contains up to 175 references, updated at 2015.}",
	archivePrefix = {arXiv},
	arxivId = {1502.03909},
	author = {Lee, Kyu Min and Min, Byungjoon and Goh, Kwang Il},
	doi = {10.1140/epjb/e2015-50742-1},
	eprint = {1502.03909},
	file = {:home/jmsallan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee, Min, Goh - 2015 - Towards real-world complexity an introduction to multiplex networks.pdf:pdf},
	isbn = {1434-6028},
	issn = {14346036},
	journal = {European Physical Journal B},
	keywords = {Statistical and Nonlinear Physics},
	mendeley-groups = {olordan},
	month = {feb},
	number = {2},
	pages = {48},
	title = {{Towards real-world complexity: an introduction to multiplex networks}},
	url = {http://link.springer.com/10.1140/epjb/e2015-50742-1},
	volume = {88},
	year = {2015}
}

