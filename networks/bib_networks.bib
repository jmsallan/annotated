@article{Guimera2005,
	abstract = {We analyze the global structure of the worldwide air transportation network, a critical infrastructure with an enormous impact on local, national, and international economies. We find that the worldwide air transportation network is a scale-free small-world network. In contrast to the prediction of scale-free network models, however, we find that the most connected cities are not necessarily the most central, resulting in anomalous values of the centrality. We demonstrate that these anomalies arise because of the multicommunity structure of the network. We identify the communities in the air transportation network and show that the community structure cannot be explained solely based on geographical constraints and that geopolitical considerations have to be taken into account. We identify each city's global role based on its pattern of intercommunity and intracommunity connections, which enables us to obtain scale-specific representations of the network.},
	annote= "{In this first analysis of the worldwide airport network, it is found that its degree distribution has a truncated power-law decaying tail, and that it has the small-world property. The existence of nodes with small degree and high betweenness is a specific property of this network. These anomalies exist because they act as hubs between their communities and the rest of the network. Authors identify the communities with a simulated annealing algorithm, and define the role of each node using two parameters: within-module degree and participation coefficient. Based on these values, they identify up to seven universal roles, that account for the multi-community structure of the network.}",
	author = {Guimer{\`{a}}, Roger and Mossa, S and Turtschi, A and Amaral, Lu{\'{i}}s a Nunes},
	doi = {10.1073/pnas.0407994102},
	file = {:Users/jmsallan/Library/Application Support/Mendeley Desktop/Downloaded/Guimer{\`{a}} et al. - 2005 - The worldwide air transportation network Anomalous centrality, community structure, and cities' global roles.pdf:pdf},
	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	keywords = {airport,network,networks},
	mendeley-groups = {olordan,AirportNetworks},
	mendeley-tags = {networks},
	month = {may},
	number = {22},
	pages = {7794--9},
	pmid = {15911778},
	title = {{The worldwide air transportation network: Anomalous centrality, community structure, and cities' global roles.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1142352{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {102},
	year = {2005}
}

